  "namespace": {
    "st": "https://onedm.org/smartthings/"
  }, 
  "defaultnamespace": "st", 

  "sdfObject": {
    "Switch": {
      "sdfProperty": {
        "value": {
          "sdfRef": "st:#/sdfData/SwitchState"
        }
      }, 
      "odmAction": {
        "on": {}, 
        "off": {},
        "toggle": {}
      }
    }
  },
  "sdfData": {
    "SwitchState": {
      "sdfChoice": {
        "on": {
          "default": "on"
        },
        "off": {
          "default": "off"
        }
      }
    }
  }
}