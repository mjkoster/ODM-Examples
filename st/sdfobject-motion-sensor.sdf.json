  }, 
  "namespace": {
    "ex": "https://onedm.org/exploratory/"
  }, 
  "defaultNamespace": "ex", 
  "sdfObject": {
    "motionSensor": {
      "label": "Motion Sensor",
      "sdfRequired": [
        "#/sdfObject/motionSensor/sdfProperty/value"
      ],
      "sdfProperty": {
        "value": {
          "sdfRef": "#/sdfData/activityState"
        }
      }, 
      "sdfEvent": {
        "stateChange": { 
          "sdfOutputData" : {
            "type": "object",
            "required": [ "activityState" ],
            "properties": {
              "activityState": { 
                "sdfRef": "#/sdfData/activityState" 
              }
            }
          }
        }
      }
    }
  },
  "sdfData": {
    "activityState": {
      "sdfChoice": {
        "active": {
          "description": "The active state",
          "type": "string",
          "default": "active"
        },
        "inactive": {
          "description": "The inactive state",
          "type": "string",
          "default": "inactive"  
        }
      }
    }
  }
}
