  "namespace": {
    "ex": "https://onedm.org/exploratory/",
    "pg": "https://onedm.org/playground/"
  }, 
  "defaultNamespace": "ex", 

  "sdfObject": {
    "ovenMode": {
      "label": "Oven Mode",
      "required": [
        "sdfProperty/ovenMode",
        "sdfProperty/supportedOvenModes",
        "sdfAction/setOvenMode"
      ],
      "sdfProperty": {
        "ovenMode": {
          "ref": "ex:#/sdfData/OvenMode"
        },
        "supportedOvenModes": {
          "type": "array",
          "items": {
            "ref": "ex:#/sdfData/OvenMode"
          }
        }
      }, 
      "sdfAction": {
        "setOvenMode": { 
          "description": "Action to set the oven mode",
          "label": "Set Oven Mode",
          "required": [ "inputData/ovenMode" ],
          "inputData": {
            "ovenMode": {
              "ref": "ex:#/sdfData/OvenMode"
              }
          }
        }
      }
    }
  }
}