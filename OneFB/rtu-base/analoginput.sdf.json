  "namespace": {
    "fb": "https://onedm.org/exploratory/onefb/"
  }, 
  "defaultNamespace": "fb", 
  
  "sdfObject": {
    "AnalogInput": {
      "sdfProperty": {
        "ScaleMinimum": {
          "type": "number",
          "default": 0
        },
        "ScaleMaximum": {
          "type": "number",
          "default": 100
        },
        "Units": {
          "type": "string",
          "default": "%"
        },
        "CurrentValue": {
          "type": "float"
        },
        "ValueType": {
          "sdfRef": "/#/sdfData/ValueType/sdfChoice/FloatValue"
        },
        "Description": {
          "type": "string"
        },
        "ObjectTypeID": {
          "const": "4904"
        },
        "ObjectInstanceID": {
          "sdfRef": "/#/sdfData/ObjectResourceID"
        }
      }
    }
  }
}  