{
  "info": {
    "title": "Example BT Mesh Sensor Setting Server Model", 
    "version": "20200710", 
    "copyright": "Copyright 2019 Michael Koster. All rights reserved.", 
    "license": "https://github.com/one-data-model/oneDM/blob/master/LICENSE"
  }, 
  "namespace": {
    "pg": "https://onedm.org/playground/"
  }, 
  "defaultnamespace": "pg", 
  "sdfObject": {
    "SensorSetting": { 
      "sdfProperty": {
        "SensorPropertyID": {
          "sdfRef": "#/sdfData/PropertyIdType"
        }, 
        "SensorSettingPropertyID": {
          "sdfRef": "#/sdfData/PropertyIdType"
        }, 
        "SensorSettingAccess": {
          "enum": [
            "read", 
            "readwrite"
          ], 
          "widthInBits": 8
        }, 
        "SensorSettingRaw": {
          "sdfRef": "#/sdfObject/SensorSetting/sdfData/SensorSettingRawValueType"
        }
      }, 
      "sdfData": {
        "SensorSettingRawValueType": {}
      }
    }, 
    "SensorCadence": {
      "sdfProperty": {
        "SensorPropertyID": {
          "sdfRef": "#/sdfData/PropertyIdType"
        }, 
        "FastCadencePeriodDivisor": {
          "type": "number", 
          "minimum": 0, 
          "maximum": 15
        }, 
        "StatusTriggerType": {
          "enum": [ 
            "formattype", 
            "unitless" 
          ]
        }, 
        "StatusTriggerDeltaDown": {
          "type": "number"
        }, 
        "StatusTriggerDeltaUp": {
          "type": "number"
        }, 
        "StatusMinInterval": {
          "type": "number", 
          "multipleOf": 1, 
          "minumum": 0, 
          "maximum": 26
        }, 
        "FastCadenceLow": {
          "type": "number"
        }, 
        "FastCadenceHigh": {
          "type": "number"
        }
      }
    }
  }, 
  "sdfData": {
    "PropertyIdType": {
      "type": "number", 
      "multipleOf": 1, 
      "minumum": 1, 
      "maximum": 65535, 
    }
  }
}