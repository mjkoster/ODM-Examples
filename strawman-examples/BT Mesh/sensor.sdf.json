{
  "info": {
    "title": "Example BT Mesh Sensor Server Model", 
    "version": "20200710", 
    "copyright": "Copyright 2019 Michael Koster. All rights reserved.", 
    "license": "https://github.com/one-data-model/oneDM/blob/master/LICENSE"
  }, 
  "namespace": {
    "pg": "https://onedm.org/playground/"
  }, 
  "defaultnamespace": "pg", 
  "sdfObject": {
    "SensorDescriptor": {
      "sdfProperty": {
        "SensorPropertyID": {
          "sdfRef": "#/sdfData/PropertyIdType"
        }, 
        "SensorPositiveTolerance": {
          "type": "number", 
          "multipleOf": 1, 
          "minimum": 1, 
          "maximum": 4095
        }, 
        "SensorNegativeTolerance": {
          "type": "number", 
          "multipleOf": 1, 
          "minimum": 1, 
          "maximum": 4095
        }, 
        "SensorSamplingFunciton": {
          "enum": [
            "Unspecified", 
            "Instantaneous", 
            "ArithmeticMean", 
            "RMS", 
            "Maximum", 
            "Minimum", 
            "Accumulated", 
            "Count"
          ]
        }, 
        "SensorMeasurementPeriod": {
          "type": "number", 
          "multipleOf": 1, 
          "minimum": 1, 
          "maximum": 255
        }, 
        "SensorUpdateInterval": {
          "type": "number", 
          "multipleOf": 1, 
          "minimum": 1, 
          "maximum": 255
        }
      }
    }, 
    "SensorData": {
      "sdfProperty": {
        "SensorData": {
          "sdfRef": "#/sdfObject/SensorData/sdfData/SensorDataType"
        }
      }, 
      "sdfData": {
        "SensorDataType": {
          "type": "array", 
          "items": [
            {
              "type": "array", 
              "items": [
                {
                  "sdfRef": "#/sdfData/PropertyIdType"
                }, 
                {
                  "sdfRef": "#/sdfObject/SensorData/sdfData/SensorDataRawValueType"
                }
              ]
            }
          ]
        }, 
        "SensorDataRawValueType": {}
      }
    }
  }, 
  "sdfData": {
    "PropertyIdType": {
      "type": "number", 
      "multipleOf": 1, 
      "minumum": 1, 
      "maximum": 65535
    }
  }
}