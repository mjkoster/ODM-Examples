  }, 
  "namespace": {
    "ex": "https://onedm.org/exploratory/"
  }, 
  "defaultNamespace": "ex", 
  
  "sdfPatch": {
    "ex:#/sdfObject/Level" : {
      "id": 8,
      "sdfProperty": {
        "CurrentLevel": { "id": 0 },
        "RemainingTime" : { "id": 1 },
        "OnOffTransitionTime" : { "id": 16 },
        "etc.": {}      
      },
      "sdfAction": { 
        "MoveToLevel" : { "id": 0 },
        "Move" : { "id": 1 },
        "etc.": {}
      },
      "sdfData": { 
        "MoveStepMode": {
          "sdfChoice": {
            "Up" : {
              "id": 0,
              "const": 0
            },
            "Down" : {
              "id": 1,
              "const": 1
            }
          }
        }
      },
      "zclFeature": {
        "comment": [ "should features be managed by an SDF extension for conditional feature sets? e.g. ifdef?",
                     "can a feature set be defined by patching definitions and settings? ",
                     "should each feature be a separate patch on top of the definition (conflicts?)" ],
        "onOffCommand": [
          "ex:#/sdfObject/Level/sdfAction/MoveToLevelwithOnOff", 
          "ex:#/sdfObject/Level/sdfAction/MovewithOnOff", 
          "ex:#/sdfObject/Level/sdfAction/StepwithOnOff", 
          "ex:#/sdfObject/Level/dfAction/StopwithOnOff"
        ]
      }
    }
  }
}
