  "namespace": {
    "st": "http://example.com/capability/odm"
  }, 
  "defaultnamespace": "st", 
  "odmObject": {
    "Switch": {
      "id": 0,
      "odmProperty": {
        "value": {
          "id": 1,
          "type": "string", 
          "enum": [
            { "on": 1}, 
            { "off": 0 }
          ]
        }
      }, 
      "odmAction": {
        "on": { "id": 3 }, 
        "off": { "id": 4 }
      }
    }
  }
}