            }
          }
        },//end ThermostatProgrammingOperationMode
//
// HVA Relay Attribute Set 0x0029-0x002F Line 205 in original file.
//
        "ThermostatRunningState": {
          "name": "Thermostat Running State",
            "odmData": { 
               "name": "HeatOn",
               "$ref": "0/odmData/zclboolean"
