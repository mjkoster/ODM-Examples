        "TemperatureSetpointHold": {
          "name": "Temperature Setpoint Hold",
          "enum": [
            { "SetpointHoldOff": 0 },
            { "SetpointHoldOn":  1 },
          ]
        },//end TemperatureSetpointHold

//
