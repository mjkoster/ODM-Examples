        "RemoteSensing": {
          "name": "Remote Sensing",
          "enum": [
            { "LocalTemperatureRemote": 1 },
            { "OutdoorTemperatureRemote":  2 },
            { "OccupancyRemote": 4 },
          ]
        },//end RemoteSensing

        "ControlSequenceOfOperation": {
          "name": "Control Sequence Of Operation",
          "enum": [
            { "CoolingOnly": 0 },
            { "CoolingWithReheat":  1 },
            { "HeatingOnly": 2 },
            { "HeatingWithReheat":  3 },
            { "CoolingAndHeating4Pipes":  4 },
            { "CoolingAndHeating4PipesWithReheat":  5 },
