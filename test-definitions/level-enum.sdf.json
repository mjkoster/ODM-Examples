{
  "info": {
    "title": "Example ODM model for the ZCL Level cluster", 
    "version": "20190504", 
    "copyright": "no copyright", 
    "license": "not licensed"
  }, 
  "namespace": {
    "zcl": "http://example.com/zcl/odm"
  }, 
  "defaultnamespace": "zcl", 
  "odmObject": {
    "Level": {
      "name": "Level Control", 
      "id": 8, 
      "odmRequired": [
        "#/odmObject/Level/odmProperty/CurrentLevel", 
        "#/odmObject/Level/odmAction/MoveToLevel", 
        "#/odmObject/Level/odmAction/Move", 
        "#/odmObject/Level/odmAction/Stop", 
        "#/odmObject/Level/odmAction/MoveToLevelwithOnOff", 
        "#/odmObject/Level/odmAction/MovewithOnOff", 
        "#/odmObject/Level/odmAction/StepwithOnOff", 
        "#/odmObject/Level/odmAction/StopwithOnOff"
      ],
      "odmProperty": {
        "CurrentLevel": {
          "name": "Current Level", 
          "id": 0, 
          "type": "number", 
          "widthInBits": 8, 
          "minimum": 0, 
          "maximum": 254
        }, 
        "RemainingTime": {
          "odmRef": "#/odmObject/Level/odmData/transitiontimedata", 
          "name": "Remaining Time", 
          "id": 1, 
          "default": 0
        }, 
        "OnOffTransitionTime": {
          "odmRef": "#/odmObject/Level/odmData/transitiontimedata", 
          "name": "On Off Transition Time", 
          "id": 16, 
          "default": 0
        }, 
        "OnLevel": {
          "name": "On Level", 
          "id": 17, 
          "type": "number", 
          "widthInBits": 8, 
          "minimum": 0, 
          "maximum": 255, 
          "default": 255
        }, 
        "OnTransitionTime": {
          "odmRef": "#/odmObject/Level/odmData/transitiontimedata", 
          "name": "On Transition Time",
          "id": 18
        }, 
        "OffTransitionTime": {
          "odmRef": "#/odmObject/Level/odmData/transitiontimedata", 
          "name": "Off Transition Time", 
          "id": 19
        }, 
        "DefaultMoveRate": {
          "name": "Default Move Rate", 
          "id": 20, 
          "type": "number", 
          "widthInBits": 16, 
          "minimum": 0, 
          "maximum": 254
        }
      }, 
      "odmAction": {
        "MoveToLevel": {
          "name": "Move to Level", 
          "id": 0, 
          "odmRequired": [
            "#/odmObject/Level/odmAction/MoveToLevel/odmData/level", 
            "#/odmObject/Level/odmAction/MoveToLevel/odmData/transitiontime"
          ],
          "odmInputData": [
            "#/odmObject/Level/odmAction/MoveToLevel/odmData/level", 
            "#/odmObject/Level/odmAction/MoveToLevel/odmData/transitiontime"
          ],
          "odmData": {
            "level": {
              "name": "Level", 
              "type": "number", 
              "widthInBits": 8, 
              "minimum": 0, 
              "maximum": 254
            }, 
            "transitiontime": {
              "name": "Transition Time", 
              "odmRef": "#/odmObject/Level/odmData/transitiontimedata"
            }
          }
        }, 
        "Move": {
          "name": "Move", 
          "id": 1, 
          "odmRequired": [
            "#/odmObject/Level/odmAction/Move/odmData/movemode", 
            "#/odmObject/Level/odmAction/Move/odmData/rate"
          ],
          "odmInputData": [
            "#/odmObject/Level/odmAction/Move/odmData/movemode", 
            "#/odmObject/Level/odmAction/Move/odmData/rate"
          ],
          "odmData": {
            "movemode": {
              "name": "Move mode", 
              "$ref": "#/odmObject/Level/odmData/directionData"
            }, 
            "rate": {
              "name": "Move rate", 
              "type": "number", 
              "widthInBits": 8, 
              "minimum": 0, 
              "maximum": 255
            }
          }
        }, 
        "Step": {
          "name": "Step", 
          "id": 2, 
          "odmRequired": [
            "#/odmObject/Level/odmAction/Step/odmData/stepmode",
            "#/odmObject/Level/odmAction/Step/odmData/stepsize",
            "#/odmObject/Level/odmAction/Step/odmData/transitiontime"
          ],
          "odmInputData": [
            "#/odmObject/Level/odmAction/Step/odmData/stepmode",
            "#/odmObject/Level/odmAction/Step/odmData/stepsize",
            "#/odmObject/Level/odmAction/Step/odmData/transitiontime"
          ],
          "odmData": {
            "stepmode": {
              "name": "Step mode", 
              "$ref": "#/odmObject/Level/odmData/directionData"
            }, 
            "stepsize": {
              "name": "Step size", 
              "type": "number", 
              "widthInBits": 8, 
              "minimum": 0, 
              "maximum": 255
            }, 
            "transitiontime": {
              "name": "Step Transition Time", 
              "odmRef": "#/odmObject/Level/odmData/transitiontimedata"
            }
          }
        }, 
        "Stop": {
          "name": "Stop",
          "id": 3
        }, 
        "MoveToLevelwithOnOff": {
          "odmRef": "#/odmObject/Level/odmAction/MoveToLevel", 
          "name": "Move To Level with On/Off",
          "id": 4
        }, 
        "MovewithOnOff": {
          "odmRef": "#/odmObject/Level/odmAction/Move", 
          "name": "Move with On/Off",
          "id": 5
        }, 
        "StepwithOnOff": {
          "odmRef": "#/odmObject/Level/odmAction/Step", 
          "name": "Step with On/Off",
          "id": 6
        }, 
        "StopwithOnOff": {
          "odmRef": "#/odmObject/Level/odmAction/Stop", 
          "name": "Stop with On/Off",
          "id": 7
        }
      }, 
      "odmData": {
        "transitiontimedata": {
          "type": "number", 
          "widthInBits": 16, 
          "minimum": 0, 
          "maximum": 65535, 
          "multipleOf": 1, 
          "unit": "seconds", 
          "scaleMinimum": 0, 
          "scaleMaximum": 6553.5
        },
        "directionData": {
          "odmEnum": {
            "up": {
              "type": "number",
              "widthInBits": 8,
              "const": 0
            },
            "down": {
              "type": "number",
              "widthInBits": 8,
              "const": 1
            }
          }
        }
      }
    }
  }
}